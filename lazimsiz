var sum=0;
		var hasil=1;
		Console.WriteLine("Reqemi daxil edin");
		
		string yazi=Console.ReadLine();
		var num=0;
	    num=Convert.ToInt32(yazi);
	
		
		for (int i=0; i<yazi.Length;  i++)
		{
			
			if(num%2==0)
			{
				hasil*=num;
			
			}
			
			else
			{
				sum+=num;
				
			}
			
		}
		Console.WriteLine(sum);
		Console.WriteLine(hasil);
